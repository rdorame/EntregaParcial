<p id="notice"><%= notice %></p>

<h1>Cli Sistemas</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>Password</th>
      <th>Tipo</th>
      <th>Domicilio</th>
      <th>Rfc</th>
      <th>Representante</th>
      <th>Cp</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Curp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cli_sistemas.each do |cli_sistema| %>
      <tr>
        <td><%= cli_sistema.name %></td>
        <td><%= cli_sistema.user %></td>
        <td><%= cli_sistema.password %></td>
        <td><%= cli_sistema.tipo %></td>
        <td><%= cli_sistema.domicilio %></td>
        <td><%= cli_sistema.RFC %></td>
        <td><%= cli_sistema.representante %></td>
        <td><%= cli_sistema.CP %></td>
        <td><%= cli_sistema.phone %></td>
        <td><%= cli_sistema.email %></td>
        <td><%= cli_sistema.CURP %></td>
        <td><%= link_to 'Show', cli_sistema %></td>
        <td><%= link_to 'Edit', edit_cli_sistema_path(cli_sistema) %></td>
        <td><%= link_to 'Destroy', cli_sistema, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cli Sistema', new_cli_sistema_path %>
